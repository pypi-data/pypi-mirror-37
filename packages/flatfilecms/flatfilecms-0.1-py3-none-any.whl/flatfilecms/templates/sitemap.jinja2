<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
   {%- for item in request.root.structure() %}{% if item != '/sitemap.xml' and item not in data.ignore %}
   <url>
     <loc>{{data.base_url}}{{item}}</loc>
   </url>
   {%- endif %}{%- endfor %}
</urlset>
