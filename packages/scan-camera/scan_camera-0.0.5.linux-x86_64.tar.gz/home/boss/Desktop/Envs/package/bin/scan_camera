#!/home/boss/Desktop/Envs/package/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'scan-camera==0.0.5','console_scripts','scan_camera'
__requires__ = 'scan-camera==0.0.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('scan-camera==0.0.5', 'console_scripts', 'scan_camera')()
    )
