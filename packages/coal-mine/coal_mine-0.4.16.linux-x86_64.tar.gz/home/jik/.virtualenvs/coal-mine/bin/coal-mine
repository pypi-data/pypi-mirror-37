#!/home/jik/.virtualenvs/coal-mine/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'coal-mine==0.4.16','console_scripts','coal-mine'
__requires__ = 'coal-mine==0.4.16'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('coal-mine==0.4.16', 'console_scripts', 'coal-mine')()
    )
