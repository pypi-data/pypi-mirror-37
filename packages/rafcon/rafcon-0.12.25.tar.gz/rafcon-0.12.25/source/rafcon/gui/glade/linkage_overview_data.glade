<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkTreeView" id="tree_view">
    <property name="visible">True</property>
    <property name="can_focus">True</property>
    <child>
      <object class="GtkTreeViewColumn" id="name_col">
        <property name="title">Name</property>
        <property name="sizing">autosize</property>
        <child>
          <object class="GtkCellRendererText" id="name_text"/>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkTreeViewColumn" id="data_type_col">
        <property name="title">Data Type</property>
        <property name="sizing">autosize</property>
        <child>
          <object class="GtkCellRendererText" id="data_type_text"/>
        </child>
      </object>
    </child>
  </object>
</interface>
