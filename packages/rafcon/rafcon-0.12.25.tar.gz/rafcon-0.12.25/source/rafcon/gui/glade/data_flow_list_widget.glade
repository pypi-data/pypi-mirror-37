<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkTreeView" id="tree_view">
    <property name="visible">True</property>
    <property name="can_focus">True</property>
    <property name="headers_clickable">False</property>
    <child>
      <object class="GtkTreeViewColumn" id="from_state_col">
        <property name="title">Source State</property>
        <property name="expand">True</property>
        <property name="sizing">autosize</property>
        <child>
          <object class="GtkCellRendererCombo" id="from_state_combo">
            <property name="editable">True</property>
            <property name="text_column">0</property>
          </object>
          <attributes>
            <attribute name="text">1</attribute>
          </attributes>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkTreeViewColumn" id="from_key_col">
        <property name="title">Source Port</property>
        <property name="expand">True</property>
        <property name="sizing">autosize</property>
        <child>
          <object class="GtkCellRendererCombo" id="from_key_combo">
            <property name="editable">True</property>
            <property name="text_column">0</property>
          </object>
          <attributes>
            <attribute name="text">2</attribute>
          </attributes>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkTreeViewColumn" id="to_state_col">
        <property name="title">Target State</property>
        <property name="expand">True</property>
        <property name="sizing">autosize</property>
        <child>
          <object class="GtkCellRendererCombo" id="to_state_combo">
            <property name="editable">True</property>
            <property name="text_column">0</property>
          </object>
          <attributes>
            <attribute name="text">3</attribute>
          </attributes>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkTreeViewColumn" id="to_key_col">
        <property name="title">Target Port</property>
        <property name="expand">True</property>
        <property name="sizing">autosize</property>
        <child>
          <object class="GtkCellRendererCombo" id="to_key_combo">
            <property name="editable">True</property>
            <property name="text_column">0</property>
          </object>
          <attributes>
            <attribute name="text">4</attribute>
          </attributes>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkTreeViewColumn" id="external_col">
        <property name="title">External</property>
        <property name="expand">True</property>
        <property name="sizing">autosize</property>
        <child>
          <object class="GtkCellRendererToggle" id="external_toggle"/>
          <attributes>
            <attribute name="active">5</attribute>
          </attributes>
        </child>
      </object>
    </child>
  </object>
</interface>
