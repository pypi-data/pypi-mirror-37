{
    "_default": {
        "id": false,
        "type": false,
        "@id": "self",
        "@type": "&external_typename",
        "@view": "@view",
        "_default": false
    },
    "AgentProfile": {
        "description": true,
        "avatar_url_base": "&external_avatar_url",
        "name": "&display_name",
        "real_name": "&real_name",
        "preferred_email": false
    },
    "User": {
        "@extends": "AgentProfile",
        "creation_date": true,
        "last_login": true,
        "login_failures": false,
        "password": false,
        "verified": true
