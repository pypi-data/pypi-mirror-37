<div class="icons-content">
    <div class="mbl">
      <h3>Update font</h3>
      <p>fab devenv compile_fontello_fonts()</p>
    </div>

    {# list generated using the following command: python3 assembl/scripts/list_css_icons.py #}
    {% set icon_list = ['checkbox', 'collapse', 'delete', 'timer', 'options', 'message', 'clipboard', 'synth', 'tableofideas', 'answer', 'lab', 'add', 'arrowdown', 'expand', 'bullet-point', 'book', 'home', 'lock', 'lock-open', 'quote', 'arrowright', 'arrowleft', 'infinite', 'discuss', 'search', 'idea', 'user', 'sheet', 'heart', 'download', 'close', 'infinite-2', 'comment', 'diamond', 'add-2', 'arrowup', 'logout', 'bell', 'imported', 'ok', 'help', 'doc', 'check', 'check-empty', 'facebook', 'github', 'gplus', 'linkedin-squared', 'openid', 'twitter', 'reload', 'mail', 'circle-empty', 'dot-circled', 'graph'] %}

    <div class="gr">
      {% for icon in icon_list %}
      <div class="gu gu-1of5 gu-m-1of1 icon">
        <div class="bx tac mrs">
          <i class="icon icon-{{icon}} mbm"></i><br>
          <span class="gray">.icon-{{icon}}</span>
        </div>
      </div>
      {% endfor %}
    </div>

</div>
