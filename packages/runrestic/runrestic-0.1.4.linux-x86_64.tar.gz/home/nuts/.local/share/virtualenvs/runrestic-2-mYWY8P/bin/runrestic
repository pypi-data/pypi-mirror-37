#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'runrestic==0.1.4','console_scripts','runrestic'
__requires__ = 'runrestic==0.1.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('runrestic==0.1.4', 'console_scripts', 'runrestic')()
    )
